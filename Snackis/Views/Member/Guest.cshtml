@model Member
@{
    ViewData["Title"] = "Guest";
    
}
<h1>@ViewData["Title"] Page</h1>

<p>Here you see your information and to update them </p>

<p><b>First name: </b>@Model.FirstName</p>
<p><b>Last name: </b>@Model.LastName</p>
<p><b>Age: </b>@Model.Age</p>
<p><b>Email: </b>@Model.Email</p>
<br />
<p><b>Username: </b>@Model.UserName</p>
<p><b>Views on profile: </b>@Model.ProfileViews</p>
<p><b>Member since: </b>@Model.RegisteryDate</p>
<br />
<p><b>Total post: </b>@Model.TotalPosts</p>
<p><b>Total replies: </b>@Model.TotalReply</p>



